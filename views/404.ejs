<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('layouts/head') %>
  </head>
  <body>
    <%- include('layouts/navbar') %>
    <main id="main" class="main global-width">
      <h1 style="font-size: 70px">404</h1>
      <h2>We are sorry. The page you requested cannot be found.</h2>
    </main>
    <%- include('layouts/footer') %>
    <script>
      let p = document.createElement("p");
      p.textContent = `The page you were looking for (${location}) was not found.`;
      main.append(p);
    </script>
  </body>
</html>
